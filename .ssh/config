    User xhalasz

Host skate-owireless
    PubkeyAcceptedKeyTypes +ssh-rsa
    HostkeyAlgorithms +ssh-rsa

Host skate-wireless
    PubkeyAcceptedKeyTypes +ssh-rsa
    HostkeyAlgorithms +ssh-rsa

Host router.home.skateman.eu
    User root
    PubkeyAcceptedKeyTypes +ssh-rsa
    HostkeyAlgorithms +ssh-rsa
    ForwardAgent yes

Host hub.home.skateman.eu
    User root
    Port 22222
